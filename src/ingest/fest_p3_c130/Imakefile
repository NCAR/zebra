MFVERSION="$Id: Imakefile,v 1.4 1992-12-18 12:45:14 granger Exp $"

LIBDS = /zeb/DataStore/blocks/libds.a

LOCAL_LIBRARIES = $(LIBDS) $(LIBZEB) $(LIBRDSS) $(LIBNETCDF) $(XLIBS)

DEPLIBS = $(LIBDS) $(LIBZEB)

SRCS = p3fest_ingest.c c130fest_ingest.c interp.c acqual.c windsname.c
OBJS = interp.o

LINTOPTS = -nuvxz

DISTFILES = 

/* DEFINES = -DDEBUG */

/* CDEBUGFLAGS = -g -pg -static */
CDEBUGFLAGS = -g

INCLUDES = -I/zeb/DataStore/blocks

NormalProgramTarget(p3fest_ingest,p3fest_ingest.o $(OBJS),$(DEPLIBS),
				$(LOCAL_LIBRARIES),/**/)

NormalProgramTarget(c130fest_ingest,c130fest_ingest.o $(OBJS),$(DEPLIBS),
				$(LOCAL_LIBRARIES),/**/)

NormalProgramTargetNoInstall(acqual,acqual.o,/**/,$(LIBNETCDF),/**/)
InstallProgramWithFlags(acqual,$(BINDIR),-s)
NormalProgramTargetNoInstall(windsname,windsname.o,/**/,$(LIBNETCDF),/**/)
InstallProgramWithFlags(windsname,$(BINDIR),-s)

DistfilesTarget($(DISTFILES))

DependTarget($(SRCS))

