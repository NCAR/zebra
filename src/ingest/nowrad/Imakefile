MFVERSION="$Id: Imakefile,v 1.6 1995-07-06 04:28:05 granger Exp $

LIBS = $(DFALIBS) $(ZLIBS) $(XLIBS) $(LIBMCIDAS) $(FLIBS)
SYSLIBS = $(LIBMATH) $(LIBTERMCAP) $(LIBREGEXP)

XCOMM nvxrect.f contains the appropriate nvxeas,nvxini,nvxopt,nvxsae
XCOMM routines for nowrad.  It must be compiled and linked here.
XCOMM

SRCS = NowradIngest.c
OBJS = NowradIngest.o nvxrect.o

DISTFILES = Imakefile $(SRCS) nvxrect.f

/*! 
 *! Requires that the MCIDAS objects in ingest/mcidas be installed
 *! into the Zeb library first
 *!
 */

CompleteProgramTarget(NowradIngest,$(OBJS),$(DEPLIBS),$(LIBS),$(SYSLIBS))

LintTarget()
DependTarget()

DistfilesTarget($(DISTFILES))

