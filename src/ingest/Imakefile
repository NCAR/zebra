MFVERSION="$Id: Imakefile,v 1.3 1993-05-17 19:42:39 granger Exp $"

/*
 * All individual ingest-related routines are kept in subdirectories.  Code
 * and header files which are shared among ingest modules are kept in the
 * DataStore library.  Some modules share FORTRAN code from MCIDAS, and
 * this code is compiled in ./mcidas and installed in a library called
 * libmcidas.a; this directory is listed first so that its library will be
 * installed before compiling any other ingest programs.
 */
#define IHaveSubdirs
#define PassCDebugFlags 'CDEBUGFLAGS=$(CDEBUGFLAGS)'

#if BuildIngestScheduler
IS=is
#endif
#if BuildClassIngest
CLASS=class
#endif
#if BuildRadarIngest
RADAR=radar
#endif
#if BuildRadarLimits
RADARLIMITS=RadarLimits
#endif
#if BuildSLGrabber
SLGRABBER=SLGrabber
#endif
#if BuildAircrftIngest
AIRCRAFT=aircraft
#endif
#if BuildFMIngest
FM=fm
#endif
#if BuildLLPIngest
LLP=llp
#endif
#if BuildNatsIngest
NATS=nats
#endif
#if BuildNowradIngest
NOWRAD=nowrad
#endif
#if BuildP3Ingest
P3=p3
#endif
#if BuildPamIngest
PAM=pam
#endif
#if BuildProfsIngest
PROFS=profs
#endif
#if BuildRsgIngest	/* the rsg directory is currently ignored since */
			/*    I don't know what it does 		*/
RSG=
#endif
#if BuildSatelliteIngest
SATELLITE=satellite
#endif
#if BuildTaoIngest
TAO=tao
#endif

SUBDIRS = mcidas \
	$(AIRCRAFT) \
	$(LLP) \
	$(RADAR) \
	$(NATS) \
	$(CLASS) \
	$(PAM) \
	$(P3) \
	$(NOWRAD) \
	$(SLGRABBER) \
	$(RADARLIMITS) \
	$(SATELLITE) \
	$(TAO) \
	$(PROFS) \
	$(FM) \
	$(RSG) \
	$(IS)

DISTFILES = Imakefile
DISTSUBDIRS = mcidas aircraft llp radar nats class pam p3 nowrad \
	      SLGrabber RadarLimits satellite profs fm is tao

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
InstallLibSubdirs($(SUBDIRS))
DistfilesTarget($(DISTFILES))
DistfilesSubdirs($(DISTSUBDIRS))

