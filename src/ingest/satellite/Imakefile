MFVERSION="$Id: Imakefile,v 1.14 1997-01-21 17:42:22 granger Exp $"

LIBS = $(DFALIBS) $(ZLIBS) $(XLIBS) $(LIBMCIDAS) $(FLIBS)
SYSLIBS = $(LIBMATH) $(LIBTERMCAP) $(LIBREGEXP)

DEPLIBS = $(LIBZEB) $(LIBZEBDS) $(LIBMCIDAS)

SRCS = SatIngest.c GMSIngest.c gvarIngest.c area_dump.c areadump.c \
	GriddedGOES.c

GOES_OBJS = nvxgoes.o
GVAR_OBJS = nvxgvar.o cfd.o cfg.o cfi.o cfr.o cfz.o ddest.o edest.o dpsino.o \
	idmyyd.o iftok.o ih.o len_trim.o lwendw.o lwmop.o lwo.o nchars.o \
	sdest.o spout.o squak.o volnam.o xonoff.o \
	puc.o movbmem.o gettim.o ic.o getday.o dw.o sendtext.o laddr.o leof.o \
	iopn.o luc.o filbuf.o scra.o stc.o dc.o delfil.o ffiletoc.o

XCOMM nvxgoes.f/nvxgvar.f contain the definitions of the nvxeas, nvxini, 
XCOMM nvxopt, and nvxsae routines required by the satellite ingestors.
XCOMM They are U. of Wisconsin SSEC copyrighted code and should *not* be 
XCOMM distributed outside of NCAR/UCAR.

DISTFILES = Imakefile $(SRCS) SatIngest.state big.sat \
	    cape_sat dir_ingest goes.ini gms.ini \
	    keywords.h sat.init satheader \
	    hackgms.c harp.ini README mcidas_read.c satingest.doc

CompleteProgramTarget(SatIngest,SatIngest.o $(GOES_OBJS),
		    $(DEPLIBS),$(LIBS),$(SYSLIBS))

CompleteProgramTarget(GMSIngest,GMSIngest.o $(GOES_OBJS),
		    $(DEPLIBS),$(LIBS),$(SYSLIBS))

CompleteProgramTarget(gvarIngest,gvarIngest.o $(GVAR_OBJS),
		    $(DEPLIBS),$(LIBS),$(SYSLIBS))

CompleteProgramTarget(GriddedGOES,GriddedGOES.o $(GOES_OBJS),
		    $(DEPLIBS),$(LIBS),$(SYSLIBS))

TestProgramTarget(testSatIngest,testSatIngest.o $(GOES_OBJS),
		    $(DEPLIBS),$(LIBS),$(SYSLIBS))

TestProgramTarget(area_dump,area_dump.o,/**/,/**/,/**/)

TestProgramTarget(areadump,areadump.o,$(DEPLIBS),$(LIBS),$(SYSLIBS))

UICppLoadFileTarget(SatIngest.lf,SatIngest.state,keywords.h)
UICppLoadFileTarget(gvarIngest.lf,gvarIngest.state,keywords.h)

gvarIngest.state: SatIngest.state
	@$(RM) gvarIngest.state ; \
	$(LN) SatIngest.state gvarIngest.state

LintTarget()
DependTarget()

DistfilesTarget($(DISTFILES))

