MFVERSION="$Id: Imakefile,v 1.3 1993-07-01 20:15:44 granger Exp $"

LIBS = $(LIBZEBDS) $(LIBZEB) $(LIBRDSS) $(LIBNETCDF)
LIBMDA = $(RDSSDIR)/pam/mda/libmda.a

SRCS = pam_ingest.c daypam_ingest.c
OBJS = pam_ingest.o daypam_ingest.o

DISTFILES = Imakefile pam_ingest.c daypam_ingest.c

/*---------------------------------------------------------------------*/
#if BuildIngest && BuildPamIngest

NormalProgramTargetNoInstall(pam_ingest, pam_ingest.o,
		$(DEPLIBS), $(LIBS) $(LIBMDA) $(LIBUNP) $(XLIBS),$(SYSLIBS))

InstallProgramWithFlags(pam_ingest,$(BINDIR),-s)

NormalProgramTargetNoInstall(daypam_ingest, daypam_ingest.o,
		$(DEPLIBS), $(LIBS) $(LIBMDA) $(LIBUNP) $(XLIBS),$(SYSLIBS))

InstallProgramWithFlags(daypam_ingest,$(BINDIR),-s)

DependTarget()

LintTarget()
/*---------------------------------------------------------------------*/
#else
OmitBuildRules(PAM ingest)
#endif

DistfilesTarget($(DISTFILES))

