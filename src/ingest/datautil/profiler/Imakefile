LOCAL_LIBRARIES=$(LIBZEB) $(LIBNETCDF) $(LIBRDSS) $(XLIBS) $(FLIBS) 

CLIBS = /usr/local/lib/gcc-lib/sparc-sun-sunos4.1.2/2.2/libgcc.a

OBJS =	prof_convert.o

SRCS =	prof_convert.c

HDRS =	pc_cmds.h

DISTFILES = Imakefile $(SRCS) $(FSRCS) $(HDRS) prof_convert.state

HEADERS =

NormalProgramTarget(prof_convert,$(OBJS),$(DEPLIBS), $(LOCAL_LIBRARIES), $(SYSLIBS))

UILoadFileTarget(prof_convert.lf, prof_convert.state)

BuildIncludes($(HEADERS),.,.)

TestProgramTarget(pc_test,$(OBJS),$(DEPLIBS),$(LOCAL_LIBRARIES),$(SYSLIBS))

CflowTarget(prof_convert, $(OBJS),)

DependTarget()

LintTarget()

DistfilesTarget($(DISTFILES))

