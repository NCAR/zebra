MFVERSION="$Id: Imakefile,v 1.3 1992-11-05 20:34:03 kris Exp $"

LIBS=$(LIBZEB) $(LIBNETCDF) $(LIBRDSS)

SRCS  = convmud.c dscopy.c dump-fgp.c fix-gp.c gprotocdf.c mudtocdf.c \
	mudutil.c readgp.c cdf_interpolate.c

FSRCS = muddsk.f mudras.f

HEADERS = fixed-gp.h mudutil.h

OTHERS = mud.id gp.hdr convmud.doc

DISTFILES = Imakefile $(SRCS) $(FSRCS) $(HEADERS) dscopy.state $(OTHERS)

NormalProgramTargetNoInstall(dscopy, dscopy.o, $(DEPLIBS),
			     $(LIBS) $(XLIBS), $(SYSLIBS))
InstallProgramWithFlags(dscopy,$(BINDIR),-s)

NormalProgramTargetNoInstall(convmud,convmud.o mudutil.o, $(DEPLIBS),
			     $(LIBS), $(SYSLIBS))
InstallProgramWithFlags(convmud,$(BINDIR),-s)

NormalProgramTarget(cdf_interpolate,cdf_interpolate.o,$(DEPLIBS),$(LIBS),
		$(SYSLIBS))

NormalProgramTarget(gprotocdf,gprotocdf.o,$(DEPLIBS),$(LIBS),$(SYSLIBS))

NormalProgramTarget(fix-gp,fix-gp.o,$(DEPLIBS),$(LIBS),$(SYSLIBS))

NormalProgramTarget(mudtocdf,mudtocdf.o mudras.o,$(DEPLIBS),
		    $(LIBS) $(FLIBS), $(SYSLIBS))

UISimpleLoadFileTarget(dscopy.lf,dscopy.state)


saber_mc:
	#setopt ansi
	#load $(CFLAGS) mudtocdf.c mudras.o -Bstatic FortranLibs $(LIBS)

saber_cm:
	#setopt ansi
	#load $(CFLAGS) convmud.c mudutil.c -Bstatic $(LIBS)

saber_gp:
	#setopt ansi
	#load $(CFLAGS) gprotocdf.c -Bstatic $(LIBS)

saber_dc:
	#setopt ansi
	#load $(CFLAGS) dscopy.c -Bstatic $(LIBS)

DependTarget()

LintTarget()

DistfilesTarget($(DISTFILES))

