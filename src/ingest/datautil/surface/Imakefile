LOCAL_LIBRARIES=\
	$(LIBZEBDS) $(LIBZEB) $(LIBNETCDF) $(LIBRDSS) $(XLIBS) $(FLIBS) 

CLIBS = /usr/local/lib/gcc-lib/sparc-sun-sunos4.1.2/2.2/libgcc.a

OBJS =	surf_convert.o

SRCS =	surf_convert.c

HDRS =	sc_cmds.h

DISTFILES = Imakefile $(SRCS) $(FSRCS) $(HDRS) surf_convert.state \
	    surf.example

NormalProgramTarget(surf_convert,$(OBJS),$(DEPLIBS), $(LOCAL_LIBRARIES), 
		    $(SYSLIBS))

UILoadFileTarget(surf_convert.lf, surf_convert.state)

TestProgramTarget(pc_test,$(OBJS),$(DEPLIBS),$(LOCAL_LIBRARIES),$(SYSLIBS))

CflowTarget(surf_convert, $(OBJS), /**/)

DependTarget()
LintTarget()

DistfilesTarget($(DISTFILES))
