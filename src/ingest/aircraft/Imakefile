MFVERSION="$Id: Imakefile,v 1.3 1994-01-21 11:48:50 granger Exp $"

LIBS = $(LIBZEB) $(LIBRDSS) $(LIBNETCDF)

OBJS = ac_ingest.o
OBJS2 = ac_status.o

SRCS = ac_ingest.c ac_status.c

DISTFILES = Imakefile $(SRCS) ac_ingest.h ac_ingest.params ac_ingest.state \
	    ac_status.state status.params test.params transfile

NormalProgramTarget(ac_ingest,$(OBJS),$(DEPLIBS),$(LIBS) $(XLIBS),$(SYSLIBS))

NormalProgramTarget(ac_status,$(OBJS2),$(DEPLIBS),$(LIBS) $(XLIBS),$(SYSLIBS))

UILoadFileTarget(ac_ingest.lf,ac_ingest.state)

UILoadFileTarget(ac_status.lf,ac_status.state)

DependTarget()
LintTarget()

DistfilesTarget($(DISTFILES))

