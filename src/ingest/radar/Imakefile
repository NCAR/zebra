MFVERSION="$Id: Imakefile,v 1.3 1993-07-01 20:15:57 granger Exp $"

LIBS = $(LIBZEBDS) $(LIBZEB) $(LIBRDSS) $(LIBNETCDF) $(XLIBS)

SRCS =  radar_ingest.c mtape.c Input.c Rasterize.c StatusWidget.c \
	RadarStatus.c XDisplay.c ds_consumer.c

OBJS = radar_ingest.o mtape.o Input.o Rasterize.o StatusWidget.o

HDRS = Ethernet.h HouseKeeping.h RadarInfo.h Types.h display.h radar_ingest.h

APPDEFAULTS = radar_ingest.ad RadarStatus.ad

DISTFILES = Imakefile $(SRCS) $(HDRS) $(APPDEFAULTS) \
	    radar_ingest.state cp4.lr.params cp4.params \
	    cp4disk.p mhr.params mhrdisk.p

/*-----------------------------------------------------------------------*/
#if BuildRadarIngest && BuildIngest

NormalProgramTargetNoInstall(radar_ingest,
	$(OBJS),$(DEPLIBS),$(LIBS),$(SYSLIBS))

UILoadFileTarget(radar_ingest.lf,radar_ingest.state)

InstallProgramSetUID(radar_ingest,$(BINDIR))

InstallAppDefaults(radar_ingest)
InstallAppDefaults(RadarStatus)

TestProgramTarget(ds_consumer,ds_consumer.o,$(DEPLIBS),$(LIBS),$(SYSLIBS))

NormalProgramTarget(RadarStatus,RadarStatus.o,$(DEPLIBS),$(LIBS),$(SYSLIBS))

LintTarget()
DependTarget()
/*-----------------------------------------------------------------------*/
#else
OmitBuildRules(radar_ingest)
#endif

DistfilesTarget($(DISTFILES))

