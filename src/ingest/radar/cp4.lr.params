!
! Parameters to read in sur scans (LOWRES) from CP4.
!
set x_resolution 480
set y_resolution 480
set x_radar 240
set y_radar 240
set gmt_offset 0
set platform "cp4"
set minimum_sweep 50
set pixels_per_km 3
set ui$pager_mode false
source file /zeb/corbet/wisp.tape
field 'reflectivity' offset 4
field 'velocity' offset 2
