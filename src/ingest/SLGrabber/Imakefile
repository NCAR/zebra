MFVERSION="$Id: Imakefile,v 1.2 1993-07-01 20:15:00 granger Exp $"

LIBS = $(LIBZEB) $(LIBRDSS) $(LIBNETCDF)

SRCS = SLGrabber.c
OBJS = SLGrabber.o

HEADERS = SLGrabber.h

DISTFILES = Imakefile $(SRCS) $(HEADERS)

/*-----------------------------------------------------------------------*/
#if BuildSLGrabber && BuildIngest

NormalProgramTargetNoInstall(SLGrabber,$(OBJS),$(DEPLIBS),$(LIBS),$(SYSLIBS))
InstallProgramWithFlags(SLGrabber,$(BINDIR),-s)

BuildIncludes($(HEADERS),.,.)

DependTarget()
LintTarget()
/*-----------------------------------------------------------------------*/
#else
OmitBuildRules(SLGrabber)
#endif

DistfilesTarget($(DISTFILES))

