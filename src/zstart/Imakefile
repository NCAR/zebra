/* -*- C -*-
 * Imakefile for the zstart script
 */

DISTFILES = Imakefile zstart.sh
DISTSUBDIRS = $(SUBDIRS)

all:: zstart

zstart: zstart.sh
	$(RM) zstart
	sed -e s,##TOPDIRHERE,BaseDir, <zstart.sh >zstart
	chmod +x zstart

clean::
	rm -f zstart

InstallNamedTarget (install,zstart,$(INSTBINFLAGS),$(BINDIR),zstart)

DistfilesTarget($(DISTFILES))

