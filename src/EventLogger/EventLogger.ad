! $Id: EventLogger.ad,v 1.10 2001-11-30 00:45:07 granger Exp $
!
! Application defaults file for the EventLogger
! DO NOT MODIFY this file if this is the library installed copy!!!
!
*font:	*-courier-bold-r-normal--12-*
*eventlogger.geometry:	-25-25
*text.width:		600
*text.height:		175
*allowShellResize:	True
*resizable:		False
*borderWidth: 1
*Toggle*font:	*-helvetica-medium-r-normal--12-*
*Command*font:	*-helvetica-medium-r-normal--12-*
*MenuButton*font:	*-helvetica-medium-r-normal--12-*
*text*font:	*-courier-medium-r-normal--12-*
*text*left:		chainLeft
*text*right:		chainRight
*text*top:		chainTop
*text*bottom:		chainBottom
*text*horizDistance:	3
*text*vertDistance:	3
*foreground:		MidnightBlue
*background:		gray90
*Form.background:	cadet blue
*Command.background:	midnight blue
*Command.foreground:	yellow
*Command.borderColor:	red
*MenuButton.background:	midnight blue
*MenuButton.foreground:	yellow
*MenuButton.borderColor:	red
*MenuButton.translations:	#override \n\
	<BtnDown>:	reset() PositionAndPopupRdssMenu()
*Scrollbar*foreground:	midnight blue
*Label.background:	cadet blue
*Label.foreground:	yellow
*logfile.label:		Log File...

!
! Settings for the log file popup
!
*logpopup*logdialog.label:	Log File Settings
*logpopup*logdialog.label.width:	260
*logpopup*logdialog.dismiss.label:	Dismiss
*logpopup*logdialog.value.width:	300
*logpopup*logdialog.mask.label:		Events ->
*logpopup*logdialog.erase.label:	Erase
*logpopup*logdialog.value.translations:	#override \n\
	<Key>Return:	no-op()		 	\n\
	<Key>Linefeed:	no-op()			\n\
	Ctrl<Key>U:	beginning-of-line()kill-to-end-of-line()



