/*
 * Imakefile for the etc/icons directory.
 */

/*
 * Hell if I'm going to type in all of those damn icon names.
 */

all::

install::
	MakeDir ($(LIBDIR)/icons)
	for icon_file in [0-9a-z]* ; do \
		$(INSTALL) $${icon_file} $(LIBDIR)/icons ; \
	done

DISTFILES = Imakefile [a-z]*

DistfilesTarget($(DISTFILES))
