all::

/*
 * Install the old xhelp files.
 */
InstallLibFile ($(HELPFILES),help,.)

/*
 * Install the HTML and GIF files.
 */
InstallLibFile ($(HTMLFILES),help,.)
InstallLibFile ($(GIFFILES),help,.)

DistfilesTarget ($(DISTFILES))
