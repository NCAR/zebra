/*
 * Imakefile for the ETC directory.
 */

MAPS = cape.map cape_aircraft.map cape_ddop.map cape_llpnet.map \
	cape_roads.map cinde.map ctex.map \
	fest_array.map harp.map nowrad.map raps_county.map \
	raps_now_region.map stormfest.map stormfest_lobes.map \
	wisp.map

DISTFILES = Imakefile $(MAPS)

all::

InstallLibFile ($(MAPS),.,.)

install::
	(cd $(LIBDIR); $(RM) wisp91.map; ln -s wisp.map wisp91.map)

DistfilesTarget($(DISTFILES))

