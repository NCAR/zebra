/*
 * GP library files.
 */

LFILES = active-loc active-time color-table contour ctr-step form-utilities \
	global grid histogram interp irg_winds iss-cap iss-rass location \
	offsets optimizer projection radar range raster rings simple-color \
	simple-track skewt station thetaplot time-height track winds xsect \
	xytrace xywind xyaxes xyobs dataskip coare-fselect \
	time-height-contour annotation ctrmove
	

DISTFILES = $(LFILES) Imakefile

all::	/* nothing */

InstallLibFile ($(LFILES),gplib,..)

DistfilesTarget ($(DISTFILES))
