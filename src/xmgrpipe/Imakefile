/*
 * Imakefile for the data store dumper
 */

SRCS = xmgrpipe.c
OBJS = xmgrpipe.o

DEPLIBS = $(LIBZEB) $(LIBZEBDS)
LIBS = $(LIBZEBDS) $(LIBZEB) $(LIBRDSS) $(LIBNETCDF) $(XLIBS)
DISTFILES = $(SRCS) Imakefile

NormalProgramTarget(xmgrpipe, $(OBJS), $(DEPLIBS), $(LIBS), $(SYSLIBS))
DistfilesTarget($(DISTFILES))
DependTarget ()
LintTarget()
