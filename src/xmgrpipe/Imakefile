/*
 * Imakefile for the ds<-->ace/gr pipe interface
 */

SRCS = xmgrpipe.c
OBJS = xmgrpipe.o

DEPLIBS = $(LIBZEB) $(LIBZEBDS)
LIBS = $(LIBZEBDS) $(LIBZEB) $(LIBRDSS) $(LIBNETCDF) $(XLIBS)
SYSLIBS = $(LIBMATH) $(LIBTERMCAP) $(LIBREGEXP)
DISTFILES = $(SRCS) Imakefile

CompleteProgramTarget(xmgrpipe, $(OBJS), $(DEPLIBS), $(LIBS), $(SYSLIBS))
DistfilesTarget($(DISTFILES))
DependTarget ()
LintTarget()
