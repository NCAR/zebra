MFVERSION="$Id: Imakefile,v 1.2 1993-03-12 20:30:28 granger Exp $"

OBJS =  xhelp.o fileServices.c Directory.o DirectoryMgr.o MultiList.o \
	RegExp.o ScrolledList.o

SRCS = $(OBJS:.o=.c)

HDRS =  Directory.h DirectoryMgr.h MultiList.h MultiListP.h RegExp.h \
	ScrolledList.h ScrolledListP.h fileServices.h xhelp.h

OTHERS = xhelp.hlp zeb.hlp bitmaps xhelp.man XHelp.ad

DISTFILES = Imakefile $(SRCS) $(HDRS) $(OTHERS)

DEPLIBS =

LOCAL_LIBRARIES = $(XLIBS)

HEADERS = xhelp.h

HELPFILES = xhelp.hlp zeb.hlp

ComplexProgramTarget(xhelp)

BuildIncludes($(HEADERS),.,.)

InstallNonExec($(HELPFILES),$(LIBDIR))

InstallAppDefaults(XHelp)

DistfilesTarget($(DISTFILES))

