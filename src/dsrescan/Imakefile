/*
 * Imakefile for the data store rescan 
 */

SRCS = dsrescan.c
OBJS = dsrescan.o

LIBS = $(LIBZEBDS) $(LIBZEB) $(LIBRDSS) $(LIBNETCDF) $(XLIBS)
SYSLIBS = $(LIBMATH) $(LIBTERMCAP) $(LIBREGEXP)
DISTFILES = $(SRCS) Imakefile

CompleteProgramTarget(dsrescan, $(OBJS), $(DEPLIBS), $(LIBS), $(SYSLIBS))
DistfilesTarget($(DISTFILES))
DependTarget()
LintTarget()
