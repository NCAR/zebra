/*
 * Imakefile for the data store data destroyer
 */

SRCS = dsdelete.c
OBJS = dsdelete.o

LIBS = $(LIBZEBDS) $(LIBZEB) $(LIBRDSS) $(LIBNETCDF) $(LIBHDF) $(XLIBS)
SYSLIBS = $(LIBMATH) $(LIBTERMCAP) $(LIBREGEXP)

DISTFILES = $(SRCS) Imakefile

CompleteProgramTarget(dsdelete, $(OBJS), $(DEPLIBS), $(LIBS), $(SYSLIBS))
DistfilesTarget($(DISTFILES))
DependTarget()
LintTarget()
