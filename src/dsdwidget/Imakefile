/*
 * Imakefile for the data store dumper
 */

SRCS = dsdwidget.c
OBJS = dsdwidget.o

LIBS = $(LIBZEBDS) $(LIBZEB) $(LIBRDSS) $(LIBNETCDF) $(XLIBS) $(LIBREGEXP)
DISTFILES = $(SRCS) Imakefile dsdwidget.ad

NormalProgramTarget(dsdwidget, $(OBJS), $(DEPLIBS), $(LIBS), $(SYSLIBS))
InstallAppDefaults(dsdwidget)
DistfilesTarget($(DISTFILES))
DependTarget()
LintTarget()
