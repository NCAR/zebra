/*
 * Imakefile for the data store dumper
 */

SRCS = dsdwidget.c xncdump.c
OBJS = dsdwidget.o xncdump.o

DEPLIBS = $(LIBZEBDS) $(LIBZEB)
LIBS = $(DSLIBS) $(ZLIBS) $(XLIBS)
SYSLIBS = $(LIBMATH) $(LIBTERMCAP) $(LIBREGEXP)
DISTFILES = $(SRCS) Imakefile dsdwidget.ad

CompleteProgramTarget(dsdwidget, dsdwidget.o, $(DEPLIBS), $(LIBS), $(SYSLIBS))
CompleteProgramTarget(xncdump, xncdump.o, /**/, $(XLIBS), $(LIBMATH))

InstallAppDefaults(dsdwidget)
DistfilesTarget($(DISTFILES))
DependTarget()
LintTarget()
