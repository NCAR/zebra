/*
 * Imakefile for the last data displayer.
 */

SRCS = LastData.c
OBJS = LastData.o

LIBS = $(LIBZEBDS) $(LIBZEB) $(LIBRDSS) $(LIBNETCDF) $(XLIBS)
SYSLIBS = $(LIBMATH) $(LIBTERMCAP) $(LIBREGEXP)

APPDEFAULTS = LastData.ad

DISTFILES = $(SRCS) $(APPDEFAULTS) Imakefile

CompleteProgramTarget(LastData, $(OBJS), $(DEPLIBS), $(LIBS), $(SYSLIBS))
InstallAppDefaults(LastData)
DependTarget()

DistfilesTarget($(DISTFILES))
