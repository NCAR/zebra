/* Archiver Imakefile -*- C -*-
 * $Id: Imakefile,v 1.15 1997-06-28 16:34:16 granger Exp $
 */

SRCS = Archiver.c Database.c Arq.c
OBJS = Archiver.o Database.o
HDRS = Database.h

INCLUDES = $(XINCLUDES)
LIBS = $(DSLIBS) $(LIBZEBRA) $(XLIBS)
SYSLIBS = $(LIBNDBM) $(LIBREGEXP)
DISTFILES = $(SRCS) $(HDRS) Imakefile Archiver.ad

CompleteProgramTarget(Archiver, $(OBJS), $(DEPLIBS), $(LIBS), $(SYSLIBS))
InstallAppDefaults(Archiver)

DBOBJS = Arq.o Database.o
CompleteProgramTarget(Arq, $(DBOBJS), $(DEPLIBS), $(LIBZEBRA), $(LIBNDBM))

DependTarget()
LintTarget()

DistfilesTarget($(DISTFILES))
