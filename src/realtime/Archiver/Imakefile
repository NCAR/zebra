
SRCS = Archiver.c Database.c Arq.c
OBJS = Archiver.o Database.o

INCLUDES = $(XINCLUDES)
LIBS = $(DSLIBS) $(ZLIBS) $(XLIBS)
SYSLIBS = -lndbm $(LIBMATH) $(LIBTERMCAP) $(LIBREGEXP)
DISTFILES = $(SRCS) Imakefile Archiver.ad

CompleteProgramTarget(Archiver, $(OBJS), $(DEPLIBS), $(LIBS), $(SYSLIBS))
InstallAppDefaults(Archiver)

DBOBJS = Arq.o Database.o
CompleteProgramTarget(Arq, $(DBOBJS), $(DEPLIBS), $(LIBS), $(SYSLIBS))

DependTarget()
LintTarget()

DistfilesTarget($(DISTFILES))
