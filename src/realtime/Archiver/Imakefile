/* Archiver Imakefile -*- C -*-
 * $Id: Imakefile,v 1.18 1999-05-12 14:12:45 burghart Exp $
 */

SRCS = Archiver.c Database.c Arq.c
OBJS = Archiver.o Database.o
HDRS = Database.h

INCLUDES = $(XINCLUDES)
LIBS = $(DFALIBS) $(LIBZEBRA) $(LIBRDSSUTIL) $(XLIBS)
SYSLIBS = $(LIBNDBM) $(LIBREGEXP) $(LIBMATH)
DISTFILES = $(SRCS) $(HDRS) Imakefile Archiver.ad

CccProgramTarget(Archiver, $(OBJS), $(DEPLIBS), $(LIBS), $(SYSLIBS))
InstallAppDefaults(Archiver)

DBOBJS = Arq.o Database.o
CccProgramTarget(Arq, $(DBOBJS), $(DEPLIBS), $(LIBZEBRA), $(LIBNDBM))

DependTarget()
LintTarget()

DistfilesTarget($(DISTFILES))
