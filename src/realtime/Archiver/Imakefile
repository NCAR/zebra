
SRCS = Archiver.c
OBJS = Archiver.o

LIBS = $(LIBZEBDS) $(LIBZEB) $(LIBRDSS) $(LIBNETCDF) $(LIBHDF) $(XLIBS)
SYSLIBS = $(LIBMATH) $(LIBTERMCAP) $(LIBREGEXP)
DISTFILES = $(SRCS) Imakefile Archiver.ad

CompleteProgramTarget(Archiver, $(OBJS), $(DEPLIBS), $(LIBS), $(SYSLIBS))
InstallAppDefaults(Archiver)
DependTarget()
LintTarget()

DistfilesTarget($(DISTFILES))
