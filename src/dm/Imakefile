LIBS = $(LIBZEBDS) $(LIBZEB) $(LIBNETCDF) $(LIBRDSS) $(XLIBS)

LIBGCC = "/usr/local/lib/gcc-lib/sparc-sun-sunos4.1.2/2.2/libgcc.a"
CLIBS = $(LIBGCC)

OBJS= dm.o dm_pd.o dm_ui.o dm_color.o DialBox.o dm_pick.o dm_config.o
SRCS= dm.c dm_pd.c dm_ui.c dm_color.c DialBox.c dm_pick.c dm_config.c

HEADERS = dm.h dm_cmds.h dm_vars.h

DISTFILES = Imakefile $(SRCS) $(HEADERS) dm.widgets dm.state DisplayMgr.ad

NormalProgramTarget(dm,$(OBJS),$(DEPLIBS),$(LIBS),$(SYSLIBS))

UILoadFileTarget_2(dm.lf,dm.state,'dm-initial',dm.widgets)

InstallAppDefaults(DisplayMgr)

BuildIncludes(dm.h,.,.)

